{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Uvicorn Server",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/venv/bin/uvicorn",
            "args": [
                "project.asgi:application",
                "--host", "0.0.0.0",
                "--port", "8000",
                "--reload"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
